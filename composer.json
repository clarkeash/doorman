{
  "name": "clarkeash/doorman",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ashley Clarke",
      "email": "me@ashleyclarke.me"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "laravel/framework": "^5.6|^6.0",
    "ramsey/uuid": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Clarkeash\\Doorman\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Clarkeash\\Doorman\\Test\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^3.6",
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Clarkeash\\Doorman\\Providers\\DoormanServiceProvider"
      ],
      "aliases": {
        "Doorman": "Clarkeash\\Doorman\\Facades\\Doorman"
      }
    }
  },
  "config": {
    "preferred-install": "dist",
    "platform": {
      "php": "7.1.3"
    }
  }
}
